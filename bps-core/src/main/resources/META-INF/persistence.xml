<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="bps-core">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<non-jta-data-source>java:/jdbc/bpsDS</non-jta-data-source>
		<mapping-file>META-INF/orm.xml</mapping-file>
		<class>com.bps.core.entities.BpsUserInfo</class>
		<class>com.bps.core.entities.BpsProcess</class>
		<class>com.bps.core.entities.BpsProcessVer</class>
		<class>com.bps.core.entities.BpsLog</class>
		<class>com.bps.core.entities.BpsOrganization</class>
		<class>com.bps.core.entities.BpsOrgProc</class>
		<class>com.bps.core.entities.BpsProcTempl</class>		
		<properties/>
	</persistence-unit>
</persistence>
